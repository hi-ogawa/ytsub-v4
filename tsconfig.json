{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"compilerOptions": {
		"verbatimModuleSyntax": true,
		"exactOptionalPropertyTypes": false,
		"checkJs": false,
		"noEmit": true,
		"types": ["@types/chrome", "vite/client"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"jsx": "react-jsx"
	}
}
